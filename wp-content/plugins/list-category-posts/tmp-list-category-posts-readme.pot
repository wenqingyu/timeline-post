msgstr ""

#. Found in remaining content paragraph.
msgid "<strong>no_post_titles</strong> - If set to <code>yes</code>, no post titles will be shown. This may make sense together with <code>content=yes</code>."
msgstr ""

#. Found in remaining content paragraph.
msgid "<strong>link_titles</strong> - Option to display titles without links. If set to <code>false</code>, the post titles won't be linking to the article."
msgstr ""

#. Found in remaining content paragraph.
msgid "<strong>link_dates</strong> - Option to wrap dates with a link to the post. Set to <code>true</code> or <code>yes</code> to enable, set to <code>false</code> or <code>no</code> to disable. Defaults to <code>false</code>."
msgstr ""

